{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./*"
      ],
      "@/components/*": [
        "./components/*"
      ],
      "@/lib/*": [
        "./lib/*"
      ],
      "@/stores/*": [
        "./stores/*"
      ],
      "@/convex/*": [
        "./convex/*"
      ],
      "@reactive-resume/schema": [
        "./lib/schema"
      ],
      "@reactive-resume/schema/*": [
        "./lib/schema/*"
      ],
      "@reactive-resume/utils": [
        "./lib/utils"
      ],
      "@reactive-resume/utils/*": [
        "./lib/utils/*"
      ],
      "@reactive-resume/ui": [
        "./components/ui"
      ],
      "@reactive-resume/ui/*": [
        "./components/ui/*"
      ],
      "@reactive-resume/hooks": [
        "./lib/hooks"
      ],
      "@reactive-resume/hooks/*": [
        "./lib/hooks/*"
      ],
      "@reactive-resume/parser": [
        "./lib/parser"
      ],
      "@reactive-resume/parser/*": [
        "./lib/parser/*"
      ]
    },
    "target": "ES2017"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "convex/**/*.ts",
    ".next/dev/types/**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "rxresumeold"
  ]
}
